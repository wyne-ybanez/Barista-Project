<nav id="navigation">
  <a href="/" class="logo logo--header__nav">UB20</a>
  {% set navPages = collections.all | eleventyNavigation %}
  <ul id="menu">
    {%- for entry in navPages %}
      <li{% if entry.url == page.url %} class="active" {% endif %}>
        <a href="{{ entry.url | url }}" class="underline">{{ entry.title }}</a>
      </li>
    {%- endfor %}
  </ul>
</nav>